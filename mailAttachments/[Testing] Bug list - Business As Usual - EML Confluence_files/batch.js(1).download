                if (key2.indexOf('macro-') == 0) {
                    var name = key2.substring(6);
                    if (nonStringParameters.includes(name)) {
                        macroParameters[name] = $.parseJSON(data[key]);
                    } else {
                        macroParameters[name] = data[key];
