                case "(":
                    expression += getNextExpression(clause.substr(i+1), ')');
                    i = expression.length - 1;
                    break;
                case "'":
                    expression += getString(clause.substr(i+1), "'");
                    i = expression.length - 1;
                    break;
                case "\"":
                    expression += getString(clause.substr(i+1), "\"");
