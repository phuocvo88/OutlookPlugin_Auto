        // Add a column on the front-end
        var addColumn = function($this, e) {
            if (!spreadsheet.perms.hasPerm('ADD_COLS')) return;

            var column = {
                name: "",
                label: "New column",
                type: "TEXT",
                renderer: "link",
                viewRenderer: undefined,
                formats: {},
                width: null,
                $placeholder: $placeholder,
