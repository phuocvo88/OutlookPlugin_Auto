            span;
        for (var i = 0 ; i < totals.length ; i++) {
            span = parseInt($cells.eq(i).attr("colspan")) || 1;
            totals[i].colspan = span;
            totalSpan += span;
        }
        var diff = totalSpan - totalHeaderSpan;
        if (totals.length > 0 && diff < 0) {
            totals[0].colspan += Math.abs(diff);
