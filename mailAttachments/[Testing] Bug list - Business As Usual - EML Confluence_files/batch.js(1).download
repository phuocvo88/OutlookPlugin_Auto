            verT = version.split(/\./),
            minT = minVersion.split(/\./);

        if (verT.length < 3) return true;
        if (minT.length != 3) throw "Can't recognize version number: " + minVersion;

        for (var i = 0 ; i < 3 ; i++) {
            if (parseInt(verT[i]) < parseInt(minT[i])) return false;
